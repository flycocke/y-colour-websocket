{
  "name": "y-colour-websocket",
  "version": "1.0.1",
  "description": "Yjs websocket",
  "type": "module",
  "funding": {
    "type": "GitHub Sponsors ‚ù§",
    "url": "https://github.com/sponsors/dmonad"
  },
  "files": [
    "dist/*",
    "auth.*",
    "sync.*",
    "awareness.*",
    "bin/*",
    "src/*"
  ],
  "scripts": {
    "clean": "rm -rf dist *.d.ts */*.d.ts *.d.ts.map */*.d.ts.map",
    "dist": "rm -rf dist && rollup -c && tsc",
    "test": "npm run lint && npm run dist && node dist/test.cjs",
    "lint": "standard && tsc",
    "types": "tsc --outDir .",
    "debug": "rollup -c && concurrently 'rollup -wc' 'http-server -o test.html'",
    "preversion": "npm run dist && npm run test && npm run types && test -e dist/src/y-websocket.d.ts && test -e dist/y-websocket.cjs",
    "postpublish": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flycocke/y-colour-websocket.git"
  },
  "keywords": [
    "Yjs"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "license": "MIT",
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules"
    ]
  },
  "bugs": {
    "url": "https://github.com/flycocke/y-colour-websocket/issues"
  },
  "homepage": "https://github.com/flycocke/y-colour-websocket#readme",
  "exports": {
    "./package.json": "./package.json",
    "./sync.js": "./sync.js",
    "./dist/sync.cjs": "./dist/sync.cjs",
    "./sync": {
      "types": "./sync.d.ts",
      "module": "./sync.js",
      "import": "./sync.js",
      "require": "./dist/sync.cjs"
    },
    "./awareness.js": "./awareness.js",
    "./dist/awareness.cjs": "./dist/awareness.cjs",
    "./awareness": {
      "types": "./awareness.d.ts",
      "module": "./awareness.js",
      "import": "./awareness.js",
      "require": "./dist/awareness.cjs"
    },
    "./auth.js": "./auth.js",
    "./dist/auth.cjs": "./dist/auth.cjs",
    "./auth": {
      "types": "./auth.d.ts",
      "module": "./auth.js",
      "import": "./auth.js",
      "require": "./dist/auth.cjs"
    },
    "./bin/utils": "./bin/utils.js",
    "./bin/callback": "./bin/callback.js",
    ".": {
      "module": "./src/y-websocket.js",
      "import": "./src/y-websocket.js",
      "require": "./dist/y-websocket.cjs"
    }
  },
  "dependencies": {
    "lib0": "^0.2.42",
    "lodash.debounce": "^4.0.8",
    "y-protocols": "^1.0.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "concurrently": "^5.3.0",
    "rollup": "^2.48.0",
    "standard": "^12.0.1",
    "typescript": "^4.2.4",
    "yjs": "^13.5.6",
    "@types/node": "^18.15.0"
  },
  "peerDependencies": {
    "yjs": "^13.5.6"
  },
  "main": "auth.js",
  "bin": {
    "y-websocket-server": "./bin/server.js",
    "y-websocket-callback": "./bin/callback.js",
    "y-websocket-utils": "./bin/utils.js"
  },
  "types": "./auth.d.ts"
}
